{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "rootDir": "./src",
    "outDir": "./dist",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "typeRoots": ["node_modules/@types"],
     "types": [
      "node",
      "cacheable-request",
      "debug",
      "emscripten",
      "http-cache-semantics",
      "keyv",
      "ms",
      "responselike",
      "semver",
      "treeify",
      "validator"
    ]
  }
}
